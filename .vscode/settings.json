{
  "eslint.enable": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/windows/**": true,
    "**/ios/**": true,
    "**/android/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "yarn.lock": true,
    "package-lock.json": true,
    ".gitattributes": true,
    "packages-cache": true
  },
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "editor.tabSize": 2
}
