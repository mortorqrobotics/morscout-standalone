{
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/windows/**": true,
    "**/ios/**": true,
    "**/android/**": true,
    "**/build/**": true,
    "**/patches/**": true,
    "**/__snapshots/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "package-lock.json": true,
    ".gitattributes": true,
    "packages-cache": true,
  },
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "editor.tabSize": 2
}
