{
    "name": "morscout",
    "version": 2,
    "builds": [
        {
            "src": ".now/build_app.sh",
            "use": "@now/static-build",
            "config": {
                "distDir": "../app/build/web"
            }
        },
        {
            "src": ".now/build_server.sh",
            "use": "@now/static-build",
            "config": {
                "distDir": "../server/build"
            }
        },
        {
            "src": ".now/server.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": ".now"
        }
    ]
}